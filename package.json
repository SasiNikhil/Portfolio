{
  "name": "portfolio-os",
  "version": "1.0.0",
  "description": "Sasi Nikhil's Portfolio OS",
  "main": "backend/server.js",
  "scripts": {
    "start": "cd backend && npm start",
    "start:frontend": "cd frontend && npm start",
    "start:backend": "cd backend && npm start",
    "start:full": "concurrently \"cd backend && npm start\" \"cd frontend && npm start\"",
    "build": "cd frontend && npm run build",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install"
  },
  "keywords": ["portfolio", "windows95", "react", "nodejs"],
  "author": "Sasi Nikhil",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^7.6.0"
  }
}
